{
  "name": "phpmyadmin-honeypot",
  "version": "1.0.0",
  "author": "fivem",
  "license": "MIT",
  "description": "Sending messages to the Telegram messenger when trying to log in phpmyadmin honeypot.",
  "keywords": [
    "phpmyadmin",
    "honeypot",
    "node",
    "nodejs",
    "telegram",
    "docker",
    "notifications",
    "docker-compose"
  ],
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "watch": "nodemon ./src/index.js",
    "lint": "eslint ./src/"
  },
  "nodemonConfig": {
    "watch": [
      "src/"
    ],
    "ext": "js,json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fivemru/phpmyadmin-honeypot.git"
  },
  "bugs": {
    "url": "https://github.com/fivemru/phpmyadmin-honeypot/issues"
  },
  "homepage": "https://github.com/fivemru/phpmyadmin-honeypot",
  "devDependencies": {
    "eslint": "^7.2.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "dayjs": "^1.8.28",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "lru-cache": "^5.1.1",
    "nanoid": "^3.1.9",
    "telegraf": "^3.38.0"
  }
}
